RewriteEngine On
RewriteCond %{index} !-f
RewriteRule ^([^/]+)/$ $1.html
RewriteRule ^([^/]+)/([^/]+)/$ /$1/$2.html
RewriteCond %{index} !-f
RewriteCond %{index} !-d
RewriteCond %{https://www.kubaicode.com/anasayfa} !(\.[a-zA-Z0-9]{1,5}|/)$
RewriteRule (.*)$ /$1/ [R=301,L]


RewriteEngine on
RewriteCond %{HTTPS_HOST} ^www\.kubaicode\.com [NC]
RewriteRule ^(.*)$ https://kubaicode.com/$1 [L,R=301]


RewriteEngine on
RewriteCond %{HTTPS_HOST} ^www\.
RewriteCond %{HTTPS}s ^on(s)|off
RewriteCond http%1://%{HTTPS_HOST} ^(https?://)(www\.)?(.+)$
RewriteRule ^ %1%3%{REQUEST_URI} [R=301,L]
